<script type="text/javascript" src="http://www.google.com/jsapi"></script>
<script src="/js/calendar.js" type="text/javascript"></script>
<link rel="stylesheet" href="/css/calendar.css" />
<script type="text/javascript">
$(document).ready(function() {
	debugger;
	drawVisualization();
});
google.load('visualization', '1', {'packages': ['corechart']});
google.setOnLoadCallback(drawVisualization);
			
function drawVisualization() {
				visualization_data = new google.visualization.DataTable();
				visualization_data.addColumn('string', 'Sales');
				visualization_data.addColumn('number', 'Sales product per Day');
				visualization_data.addRow(['Product1', 11]);
				visualization_data.addRow(['Product2', 1]);
				visualization_data.addRow(['Product3', 2]);
				visualization_data.addRow(['Product4', 3]);
				visualization = new google.visualization.PieChart(document.getElementById('piechart2'));     
				visualization.draw(visualization_data, {title: 'Sales product', height: 300});       
}
			
			
</script>

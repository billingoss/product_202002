/* ========================  jQuery datepicker */
.datepicker-box { position: relative; display: inline-block; height: 42px; /* min-width: 155px;*/ background-color: #fff; }

.datepicker-box input { width: 100%; padding: 7px 35px 9px 10px; z-index: 1; background-color: transparent; }

.datepicker-box input:disabled { z-index: 0; }

.datepicker-box:after { content: ""; display: block; position: absolute; top: 9px; right: 8px; width: 22px; height: 24px; background-position: -93px 0px; }

.ui-datepicker.ui-widget { border: 1px solid #f3f3f3; padding: 0; z-index: 99999 !important; box-shadow: 0px 0px 25px 0 rgba(0, 0, 0, 0.05), 0px 0px 25px 0 rgba(0, 0, 0, 0.05); }

.ui-corner-all, .ui-corner-bottom, .ui-corner-br, .ui-corner-right { border-radius: 0; }


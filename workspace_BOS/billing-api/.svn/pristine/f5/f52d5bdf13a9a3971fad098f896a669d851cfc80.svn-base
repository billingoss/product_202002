.ui-datepicker-title { height: 40px; line-height: 38px; }

.ui-datepicker-title .ui-datepicker-month { line-height: 38px; color: #1cc381; }

.ui-datepicker-title .ui-datepicker-year { line-height: 38px; color: #1cc381; }

.ui-datepicker .ui-datepicker-header { border: 0; border-bottom: 1px solid #f3f3f3; }

